    currentRow = currentRow + 1
    ws2.Cells(currentRow, 1).Resize(1, 3).Value = Array("Nama", "Alamat", "Usia")
    currentRow = currentRow + 1
    
    ' Tulis data untuk H03
    For i = 2 To lastRow1
